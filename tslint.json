{
    "extends": "tslint:latest",
    "rules": {
        "no-var-requires": false,
        "interface-name": [
            false
        ],
        "no-empty-interface": false,
        "quotemark": [
            "single"
        ],
        "semicolon": [
            false
        ],
        "ordered-imports": [
          false
        ],
        "trailing-comma": [
          false
        ],
        "one-variable-per-declaration": [
          false
        ],
        "variable-name": [
          false
        ],
        "object-literal-sort-keys": false
    }
}
